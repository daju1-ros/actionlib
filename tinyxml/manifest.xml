<package>
  <description>
  TinyXML is a simple, small, C++ XML parser that can be easily integrating
  into other programs. 

  This packages wraps a rosdep installation of tinyxml.

  </description>
  <author>Lee Thomason</author>
  <url>http://www.grinninglizard.com/tinyxml</url>
  <license>ZLib</license>
  <review status="3rdparty doc reviewed" notes="ticket:2789"/>
  <export>
    <cpp cflags="`pkg-config --cflags tinyxml`" lflags="`pkg-config --libs tinyxml`"/>
    <doxymaker external="http://www.grinninglizard.com/tinyxml"/>
  </export>

  <rosdep name="tinyxml" />

  <platform os="ubuntu" version="9.04"/>
  <platform os="ubuntu" version="9.10"/>
  <platform os="ubuntu" version="10.04"/>
  <platform os="ubuntu" version="10.10"/>
  <platform os="ubuntu" version="11.04"/>
  <platform os="ubuntu" version="11.10"/>
</package>
